{
  "name": "@baicie/storage",
  "version": "0.0.6",
  "packageManager": "pnpm@10.26.2",
  "type": "module",
  "description": "轻量响应式数据存储代理，统一 localStorage / sessionStorage / Cookie / IndexedDB",
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage",
    "cookie",
    "indexeddb",
    "reactive",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --incremental --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
